<UserControl x:Class="projekatSIMS.UI.Dialogs.View.OwnerView.GuestReviewChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:projekatSIMS.UI.Dialogs.View.OwnerView"
             xmlns:locals="clr-namespace:projekatSIMS.UI.Dialogs.ViewModel.OwnerViewModel"

             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="180" d:DesignWidth="322">
    <UserControl.Resources>
        <DataTemplate DataType="{x:Type locals:GuestReviewChartViewModel}">
            <local:GuestReviewChart />
        </DataTemplate>
    </UserControl.Resources>
             <Grid>
                <lvc:CartesianChart  Series="{Binding ChartSeries}" />

            </Grid>
</UserControl>
